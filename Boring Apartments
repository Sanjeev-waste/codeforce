// question: https://codeforces.com/problemset/problem/1433/A
// code: https://onlinegdb.com/pZU9ygkj3
#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        string x;
        cin>>x;
        int n=x.size();
        string a="";
        a+=x[0];
        int b = stoi(a);
        int c = (b- 1)*10;
        int A=0;
        for(int i=1;i<=n;i++){
            A+=i;
        }
        cout<<A+c<<endl;
    }
    
}
