question: https://codeforces.com/contest/1711/problem/A
code: https://onlinegdb.com/9xGpWBMiy

#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++){
            arr[i]=i+ 1;
        }
        rotate(arr, arr+ 1, arr+ n);
        for(int i=0;i<n;i++){
            cout<<arr[i]<<" ";
        }
        cout<<endl;
    }
}
