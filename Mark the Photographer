#include<iostream>
#include<algorithm>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int n,x;
        cin>>n>>x;
        int m=2*n;
        int arr[m];
        for(int i=0;i<m;i++){
            cin>>arr[i];
        }
        sort(arr, arr+ m);
        bool A=0;
        for(int i=0;i<n;i++){
            int a=abs(arr[n+i]-arr[i]);
            if(a>=x){
                
            }
            else{
                A=1;
            }
        }
        if(A==1){
            cout<<"NO"<<endl;
        }
        else{
            cout<<"YES"<<endl;
        }
    }
}
